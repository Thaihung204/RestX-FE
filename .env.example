# Backend API Configuration
NEXT_PUBLIC_API_URL=http://localhost:5000/api
# hoặc https://localhost:7000/api nếu dùng HTTPS

# Optional: API Key nếu backend yêu cầu
NEXT_PUBLIC_API_KEY=

# Optional: Timeout cho API calls (milliseconds)
NEXT_PUBLIC_API_TIMEOUT=30000

# Authentication
NEXT_PUBLIC_AUTH_ENDPOINT=/auth
NEXT_PUBLIC_TOKEN_KEY=restx_token
